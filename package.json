{
  "name": "chai-structured-like",
  "version": "1.0.0",
  "description": "A Chai plugin with chainable helper assertions, comparing the structure of two objects",
  "scripts": {
    "test": "mocha test/**/*.test.js --ui exports --recursive --reporter spec --check-leaks",
    "cover": "nyc --check-coverage --lines 90 npm test",
    "coveralls": "npm run cover && nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xasdx/chai-structured-like.git"
  },
  "keywords": ["chai", "plugin", "structure", "object", "shape", "compare", "assert", "expect"],
  "author": "xasdx",
  "license": "MIT"
}
